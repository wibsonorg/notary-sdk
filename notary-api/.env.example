NODE_ENV=development
HOST=0.0.0.0
PORT=9200

LOG_TYPE=combined
ERROR_LOG=/path/to/error.log
COMBINED_LOG=/path/to/combined.log

CACHE=enabled
CACHE_ADAPTER=memory

NOTARY_SIGNING_SERVICE_URI='http://localhost:9201'
NOTARY_VALIDATOR_URI='http://localhost:9300'
TIMEOUT=1000

DATA_EXCHANGE_ADDRESS='0xcc5d6900b473c7fe4dfd542472bad2a3f76b8c24'
WEB3_PROVIDER='wss://ropsten.infura.io/ws'
REDIS_SOCKET='/var/run/redis/redis.sock'

RESPONSES_PERCENTAGE=30

# NOTARIZATION_FEE_PERCENTAGE expressed as integer
NOTARIZATION_FEE_PERCENTAGE=10

NOTARIZATION_TERMS_OF_SERVICE='Terms of Service'

NOTARIZATION_RESULTS_STORE_PATH='/tmp/notarization_results'
DATA_VALIDATION_RESULTS_STORE_PATH='/tmp/data_validation_results'
LEVEL_DIRECTORY=/tmp

TAKE_DATA_FROM_STORAGE=false
STORAGE_URL='https://storage.com'
STORAGE_REGION='storeageRegion'
STORAGE_BUCKET='storageBucket'
STORAGE_USER='storageUsr'
STORAGE_PASSWORD='storagePwd'

EVENT_SUBSCRIBERS_INTERVAL=600000
EVENT_SUBSCRIBERS_LAST_PROCESSED_BLOCK=4304982
