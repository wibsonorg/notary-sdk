NODE_ENV=development
HOST=0.0.0.0
PORT=9200

LOG_TYPE=combined
ERROR_LOG=/path/to/error.log
COMBINED_LOG=/path/to/combined.log

CACHE=enabled
CACHE_ADAPTER=memory

NOTARY_SIGNING_SERVICE_URI='http://localhost:9201'
NOTARY_VALIDATOR_URI='http://localhost:9300'
TIMEOUT=1000
WEB3_PROVIDER='wss://ropsten.infura.io/ws'
