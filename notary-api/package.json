{
  "name": "notary-api",
  "version": "1.0.2",
  "description": "Wibson Notary API",
  "author": "Wibson Development Team",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src -d dist",
    "serve": "node dist/index.js",
    "start": "nodemon src/index.js",
    "test": "ava --tap | faucet",
    "test:watch": "ava --watch",
    "test:coverage": "nyc npm test",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "publish": "scripts/publish.sh $npm_package_name"
  },
  "dependencies": {
    "apicache": "^1.2.3",
    "async-redis": "^1.1.4",
    "aws-sdk": "^2.188.0",
    "axios": "^0.18.0",
    "babel-polyfill": "6.26.0",
    "bignumber.js": "^7.2.1",
    "body-parser": "^1.18.3",
    "cors": "^2.8.4",
    "dotenv": "^6.0.0",
    "eth-crypto": "1.2.3",
    "express": "^4.16.3",
    "express-boom": "^2.0.0",
    "glob": "^7.1.2",
    "helmet": "^3.12.1",
    "level": "^4.0.0",
    "morgan": "1.9.1",
    "node-forge": "^0.7.1",
    "openpgp": "3.1.0",
    "read": "1.0.7",
    "redis": "^2.8.0",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "swagger-jsdoc": "^1.9.7",
    "swagger-ui-express": "^3.0.10",
    "truffle-contract": "3.0.6",
    "uuid": "^3.3.2",
    "web3": "1.0.0-beta.33",
    "winston": "^3.0.0"
  },
  "devDependencies": {
    "ava": "^0.25.0",
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-register": "^6.26.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "faucet": "0.0.1",
    "ganache-cli": "^6.1.3",
    "level-test": "^4.0.0",
    "nodemon": "^1.18.9",
    "nyc": "12.0.2",
    "redis-mock": "^0.27.0",
    "sinon": "^6.1.3",
    "supertest": "^3.1.0",
    "testdouble": "^3.8.1"
  },
  "nodemonConfig": {
    "exec": "clear;babel-node",
    "watchedExtensions": [
      "js"
    ],
    "ignore": [
      "__mocks__/*",
      "__test__/*",
      "*.test.js"
    ],
    "delay": "7000"
  },
  "nyc": {
    "check-coverage": true,
    "lines": 50,
    "cache": false,
    "reporter": [
      "html",
      "text",
      "lcov"
    ],
    "exclude": [
      "src/utils/logger.js"
    ]
  },
  "ava": {
    "files": [
      "test/**/*.test.js"
    ],
    "cache": true,
    "concurrency": 5,
    "failWithoutAssertions": false,
    "tap": true,
    "verbose": true,
    "compileEnhancements": false,
    "require": [
      "babel-register",
      "babel-polyfill",
      "./test/setup"
    ]
  }
}
