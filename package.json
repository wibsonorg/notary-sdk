    "ioredis": "^3.2.2",
    "jsonwebtoken": "^8.2.1",
    "jsrsasign": "^8.0.12",
    "level": "^2.1.2",
    "morgan": "^1.9.0",
    "ramda": "^0.25.0",
    "sha256": "^0.2.0",
    "truffle-contract": "^3.0.2",
    "uuid": "^3.2.1",
    "web3": "1.0.0-beta.27",
    "winston": "^2.4.0"
